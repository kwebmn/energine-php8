<?xml version="1.0" encoding="UTF-8" ?>
<configuration>
    <state name="main">
        <javascript>
            <behavior name="UserProfile" />
        </javascript>
        <fields>
            <field name="u_id"></field>
            <field name="u_password"></field>
            <field name="u_password2"></field>
        </fields>
        <toolbar>
            <control id="change" title="BTN_CHANGE" type="submit" />
        </toolbar>
        <translations>
            <translation const="TXT_PROFILE_CHANGE_PASSWORD" />
        </translations>
    </state>
    <state name="save">
        <uri_patterns>
            <pattern>/save/</pattern>
        </uri_patterns>
    </state>
</configuration>